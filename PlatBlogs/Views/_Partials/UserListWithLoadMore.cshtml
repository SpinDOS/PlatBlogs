@model PlatBlogs.Views._Partials.UserListWithLoadMoreModel
@if (Model.Users.Count > 0)
{
    foreach (var user in Model.Users)
    {
        @await Html.PartialAsync("~/Views/_Partials/UserView.cshtml", user)
    }
}
else
{
    @(Model.DefaultText)
}

@if (Model.MoreUsersExist)
{
    @await Html.PartialAsync("~/Views/_Partials/LoadMore.cshtml", Model.LoadMoreModel)
}
